---
import { menuItems } from "../consts";
---

<nav
  class="sticky top-0 flex justify-center b-4 px-6 py-4 bg-slate-700 text-slate-100 rounded-full w-1/3 mx-auto min-w-fit shadow-2xl shadow-slate-950"
>
  <ul class="flex items-center gap-2">
    {
      menuItems.items.map((data) => (
        <li class="">
          <a href={data.url} class="px-4 h-10 hover:underline">
            {data.name}
          </a>
        </li>
      ))
    }
  </ul>
</nav>
