---
import { menuItems } from "../consts";
---

<nav class="flex justify-center px-6 py-4 bg-slate-950 text-slate-100 rounded-full md:w-2/3 lg:w-1/3 mx-auto">
    <ul class="flex items-center gap-4 ">
      {
          menuItems.items.map((data) => (
            <li class="hover:font-bold" ><a href={data.url} class="px-4 h-10 hover:font-bold">
              {data.name}
            </a></li>
          ))
        }
    </ul>
  </nav>


